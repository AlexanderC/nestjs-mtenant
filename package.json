{
  "name": "nestjs-mtenant",
  "version": "1.0.0",
  "private": false,
  "description": "NestJS module for multi-tenancy with deep integration into Sequelize and TypeORM",
  "keywords": [
    "nest",
    "tenant",
    "multitenant",
    "whitelabel",
    "namespace",
    "typeorm",
    "sequelize"
  ],
  "homepage": "https://github.com/AlexanderC/nestjs-mtenant#readme",
  "bugs": {
    "url": "https://github.com/AlexanderC/nestjs-mtenant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderC/nestjs-mtenant.git"
  },
  "license": "MIT",
  "author": "Alex Cucer<alexander.moldova@gmail.com>",
  "contributors": [
    "Alex Cucer <alexander.moldova@gmail.com>"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "deploy": "sh ./publish.sh",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\"",
    "release": "npx standard-version",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "patch": "npm run release -- --release-as patch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "nestjs-cls": "^6.0.0",
    "reflect-metadata": ">=0.1.13"
  },
  "peerDependenciesMeta": {
    "@nestjs/swagger": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "sequelize": {
      "optional": true
    },
    "sequelize-typescript": {
      "optional": true
    },
    "typeorm": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "ioredis": "^5.4.0",
    "sequelize": "^6.37.0",
    "sequelize-typescript": "^2.1.6",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-angular": "^19.0.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "nestjs-cls": "^6.0.0",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.7.0"
  }
}
